.aboutus{
	width: 100%;
}
.aboutus .box1{
	background: #fff;
	height: 430px;
	position: relative;
}
.aboutus .box1 .left{
	position: absolute;
	left: 0;
}
.aboutus .box1 .left h1{
	height: 60px;
    line-height: 60px;
    font-size: 32px;
    font-weight: bold;
    color: #666666;
}
.aboutus .box1 .left p{
	width: 650px;
	line-height: 30px;
	font-size: 16px;
	color: #828181;
}
.aboutus .box1 .right{
	position: absolute;
	right: 0;
}
