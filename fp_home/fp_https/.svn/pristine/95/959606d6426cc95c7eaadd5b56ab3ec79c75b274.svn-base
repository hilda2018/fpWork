/*首页*/
.main {
	width: 1190px;
	padding-top: 15px;
}

.main .category {
	width: 250px;
	float: left;
}

.main .category h4 {
	color: #ff771c;
	font-size: 16px;
	height: 30px;
	font-weight: bold;
	background: url(../images/category.png) 0px top no-repeat;
	padding-left: 30px;
}

.main .category ul {
	width: 230px;
	border-bottom: 1px dashed #e6e6e6;
	margin-right: 20px;
}

.main .category ul li a {
	width: 115px;
	height: 40px;
	font-size: 16px;
	line-height: 40px;
	text-align: left;
	color: #333;
	float: left;
}

.main .category ul li a.more {
	color: #00a0ea;
}

.main .category ul li a em {
	width: 8px;
	height: 4px;
	background: url(../images/arrow_d.png) no-repeat;
	vertical-align: middle;
	display: inline-block;
	margin-left: 8px;
}

.main .category ul.last {
	border-bottom: none;
}

.main .banner {
	width: 470px;
	float: left;
}

.main .banner .banner-pic {
	width: 470px;
	height: 420px;
	display: block;
}

.main .banner .visible-info {
	width: 470px;
}

.main .banner .visible-info li.first {
	width: 204px;
	margin-top: 20px;
	float: left;
	padding-right: 20px;
	border-right: 1px solid #e6e6e6;
}

.main .banner .visible-info li.last {
	width: 204px;
	margin-top: 20px;
	float: left;
	padding-left: 20px;
}

.main .banner .visible-info li h3 {
	color: #333;
	font-size: 16px;
}
/*.main .banner .visible-info li .detail{
	width: 150px;
	height: 100px;
	float: left;
}*/
.main .banner .visible-info li span {
	color: #999;
	font-size: 14px;
	line-height: 20px;
	width: 234px;
	height: 40px;
	display: block;
	margin-top: 10px;
}

.main .banner .visible-info li .detail {
	height: 70px;
}

.main .banner .visible-info li .detail .visit {
	color: #00a0ea;
	font-size: 14px;
	display: inline-block;
	margin-top: 56px;
}

.main .banner .visible-info li .detail img {
	width: 90px;
	height: 70px;
	vertical-align: bottom;
	float: right;
}

.main .sales-info {
	width: 453px;
	height: 577px;
	border: 1px solid #e6e6e6;
	float: right;
}

.main .sales-info .import {
	width: 453px;
	height: 414px;
	border-bottom: 1px solid #e6e6e6;
}

.main .sales-info .import h1 {
	text-align: center;
	color: #000;
	font-size: 26px;
	height: 110px;
	line-height: 110px;
}

.main .sales-info .import .detail {
	padding: 0 25px 25px 25px;
	width: 403px;
	height: 284px;
}

.main .sales-info .import .detail li {
	width: 201px;
	height: 92px;
	float: left;
	margin-bottom: 50px;
}

.main .sales-info .import .detail li img {
	width: 92px;
	height: 92px;
	float: left;
	display: inline-block;
}

.main .sales-info .import .detail li .info {
	width: 99px;
	height: 92px;
	float: right;
	padding: 0 5px;
}

.main .sales-info .import .detail li .info h3 {
	height: 30px;
	line-height: 30px;
	font-size: 20px;
	color: #333;
}

.main .sales-info .import .detail li .info span {
	color: #999;
	line-height: 20px;
	height: 40px;
}

.main .sales-info .news {
	width: 453px;
	padding: 0 25px;
}

.main .sales-info .news h1 {
	width: 383px;
	height: 40px;
	line-height: 40px;
	color: #333;
	padding: 0 10px;
	font-size: 21px;
}

.main .sales-info .news h1 .more {
	font-size: 14px;
	color: #0d90e4;
	float: right;
	font-weight: normal;
}

.main .sales-info .news li a {
	width: 383px;
	padding: 0 10px;
	height: 40px;
	line-height: 40px;
	border-bottom: 1px dashed #e6e6e6;
	display: block;
	color: #666;
	font-size: 14px;
}

.main .sales-info .news li a.last {
	border-bottom: none;
}

.center {
	width: 1190px;
	padding-top: 40px;
}

.center .list-box {
	width: 1188px;
	height: 270px;
	border: 1px solid #e6e6e6;
	margin-bottom: 40px;
}

.center .list-box .left {
	border-right: 1px solid #e6e6e6;
	width: 201px;
	height: 232px;
	float: left;
	padding: 19px;
}

.center .list-box .left h3 {
	height: 40px;
	font-size: 27px;
	color: #333;
}

.center .list-box .left span {
	color: #999;
	line-height: 20px;
	height: 40px;
	font-size: 14px;
}

.center .list-box .left a {
	color: #00a0ea;
	font-size: 16px;
	display: block;
	margin-top: 30px;
}

.center .list-box .right {
	width: 948px;
	height: 270px;
	float: right;
	position: relative;
}

.center .list-box .right .prev {
	position: absolute;
	display: block;
	height: 27px;
	width: 13px;
	top: 50%;
	left: 33px;
	margin-top: -13px;
	background: url(../images/arrow_l.png) no-repeat;
}

.center .list-box .right .next {
	position: absolute;
	display: block;
	height: 27px;
	width: 13px;
	top: 50%;
	right: 33px;
	margin-top: -13px;
	background: url(../images/arrow_r.png) no-repeat;
}

.center .list-box .right .box {
	width: 825px;
	height: 270px;
	margin: 0 auto;
	overflow: hidden;
	position: relative;
}

.center .list-box .right .box .piclist1 {
	height: 270px;
	position: absolute;
	left: 0;
}

.center .list-box .right .piclist1 li {
	width: 140px;
	height: 246px;
	padding: 12px 12px 12px 13px;
	float: left;
}

.center .list-box .right .piclist1 li a {
	display: block;
}

.center .list-box .right .piclist1 li a img {
	width: 140px;
	height: 140px;
	display: block;
}

.center .list-box .right .piclist1 li a .name {
	margin: 10px 0 5px;
	height: 40px;
	line-height: 20px;
	color: #999;
	font-size: 14px;
}

.center .list-box .right .piclist1 li a .time {
	height: 30px;
	line-height: 30px;
	color: #ff8a00;
	font-size: 16px;
	font-weight: bold;
}

.center .list-box .right.buyer .piclist1 li a .name {
	height: 30px;
	line-height: 30px;
	color: #333;
	font-size: 16px;
	font-weight: bold;
}

.center .list-box .right.buyer .piclist1 li a .time {
	line-height: 20px;
	color: #999;
	font-size: 14px;
	font-weight: normal;
}

.center .list-box .service .box li {
	width: 160px;
	height: 246px;
	padding: 12px 23px;
	float: left;
}

.center .list-box .service .box li a {
	display: block;
}

.center .list-box .service .box li a img {
	width: 160px;
	height: 160px;
	display: block;
}

.center .list-box .service .box li a .name {
	height: 40px;
	line-height: 20px;
	color: #333;
	font-size: 16px;
	font-weight: normal;
}

.center .list-box .service .box li a.view {
	height: 30px;
	line-height: 30px;
	color: #00a0ea;
	font-size: 14px;
	font-weight: normal;
}

.center .link {
	width: 1188px;
	height: 230px;
	border: 1px solid #e6e6e6;
	margin-bottom: 40px;
}

.center .link h1 {
	height: 65px;
	line-height: 65px;
	padding-left: 20px;
	color: #000;
	font-size: 27px;
}

.center .link ul {
	width: 1186px;
	padding: 0 1px;
}

.center .link ul li {
	width: 200px;
	height: 110px;
	padding: 0 18px;
	float: left;
	border-right: 1px solid #e6e6e6;
	margin-top: 10px;
}

.center .link ul li h3 {
	height: 40px;
	line-height: 40px;
	font-size: 18px;
	font-weight: normal;
}

.center .link ul li h3 img {
	height: 25px;
	vertical-align: middle;
	margin-right: 10px;
}

.center .link ul li p {
	margin-top: 10px;
	line-height: 20px;
	font-size: 14px;
	color: #999;
}

.center .link ul li.last {
	border-right: none;
}

/*找回密码*/
.account-info {
	width: 1188px;
	border: 1px solid #cdcdcd;
	margin: 80px auto 200px;
	box-shadow: 0 0 10px #ddd;
}

.account-info h3 {
	height: 54px;
	line-height: 54px;
	text-align: center;
	font-size: 20px;
	color: #ff7719;
	background: #f8f8f8;
	border-bottom: 1px solid #ccc;
}

.account-info .detail {
	padding: 120px 0px;
}

.account-info .detail p {
	height: 50px;
	line-height: 50px;
}

.account-info .detail p span {
	width: 480px;
	display: inline-block;
	vertical-align: middle;
	height: 50px;
	line-height: 50px;
	text-align: right;
	font-size: 16px;
	padding-right: 10px;
}

.account-info .detail p input {
	height: 33px;
	line-height: 33px;
	border: 1px solid #c2c2c2;
	font-size: 16px;
	padding-left: 10px;
	vertical-align: middle;
}

.account-info .detail p .code {
	width: 101px;
	height: 31px;
	padding-left: 10px;
	vertical-align: middle;
}

.account-info .detail p .forget {
	text-decoration: underline;
	color: #00a0ea;
	margin-left: 10px;
	font-size: 16px;
}

.account-info .detail p .btn {
	width: 180px;
	height: 35px;
	line-height: 35px;
	color: #fff;
	background: #ff751a;
	text-align: center;
	font-size: 16px;
	display: inline-block;
	margin-top: 60px;
}